{
    "name": "plugin_name",
    "version": "1.0.0",
    "type": "module",
    "description": "A WordPress boilerplate plugin using vue.js",
    "scripts": {
        "replace:dev": "ts-node --esm ./src/env/development_mode.ts",
        "replace:prod": "ts-node --esm ./src/env/production_mode.ts",
        "watch": "pnpm run dev",
        "production": "pnpm run build",
        "\n========== BUILD >> ==========": "",
        "build": "pnpm build:ts && pnpm replace:prod && vite build",
        "build:ts": "tsc --build",
        "\n========== DEV >> ==========": "",
        "dev": "pnpm replace:dev && vite",
        "\n========== TEST >> ==========": "",
        "\n========== PUBLISH >> ==========": "",
        "release": "pnpm prerelease && pnpm publish --access public --report-summary --no-git-checks",
        "prerelease": "pnpm clean && pnpm build && pnpm run docs && pnpm fix && pnpm check && pnpm test",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "tsc --build --clean",
        "clean:build": "pnpm exec rm -R -f dist && pnpm exec rm -R -f assets",
               "\n========== CODE STYLE >> ==========": "",
        "prettier": "cross-var prettier {$npm_package_config_prettier_path}.{$npm_package_config_prettier_extensions} --write --ignore-path .gitignore --ignore-path .prettierignore",
        "\n========== CHECK >> ==========": "",
        "check": "pnpm build:ts && pnpm clean:ts",
        "\n========== FIX >> ==========": "",
        "fix": "pnpm prettier"
    },
    "main": "index.js",
    "dependencies": {
        "@wordpress/hooks": "^3.1.1",
        "autoprefixer": "^10",
        "chart.js": "2.9.4",
        "element-plus": "^1.1.0-beta.20",
        "moment": "^2.29.1",
        "tailwindcss": "^3",
        "vite-plugin-live-reload": "^3.0.1",
        "vue-router": "^4.0.11"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.19.6",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "cross-var": "^1.1.0",
        "deepmerge": "^4.3.1",
        "eslint": "^8.45.0",
        "prettier": "^3.0.0",
        "prettier-plugin-jsdoc": "^1.0.1",
        "prettier-plugin-sh": "^0.13.1",
        "@babel/preset-env": "^7.14.7",
        "@snailicide/build-config": "^0.2.5",
        "@snailicide/g-library": "^0.2.1",
        "@types/glob": "^8.1.0",
        "@types/node": "^20.5.0",
        "@unocss/reset": "^0.55.0",
        "@vitejs/plugin-vue": "^3.1.0",
        "@vue/compiler-sfc": "^3.2.20",
        "babel-eslint": "^10.1.0",
        "babel-plugin-component": "^1.1.1",
        "babel-plugin-import": "^1.13.3",
        "cross-env": "^7.0.3",
        "glob": "^10.3.3",
        "postcss": "^8.3.5",
        "resolve-url-loader": "^5.0.0",
        "rollup-plugin-copy": "^3.4.0",
        "sass": "^1.35.1",
        "sass-loader": "^10",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "unocss": "^0.55.0",
        "vite": "^4.4.9",
        "vue": "^3.3.4",
        "vue-cli-plugin-tailwind": "~3.0.0",
        "vue-loader": "^16.3.0",
        "vue-template-compiler": "^2.6.14",
        "vue-tsc": "^1.8.8"
    },
    "config": {
        "eslint": {
            "extensions": "ts,js,cjs,mjs",
            "path": ".es*,*,**/*",
            "config": ".eslintrc.cjs"
        },
        "prettier": {
            "extensions": "php,ts,js,cjs,mjs,cts,json,xml,yaml,yml,graphql,md",
            "path": "**/*,*"
        }
    }
}
